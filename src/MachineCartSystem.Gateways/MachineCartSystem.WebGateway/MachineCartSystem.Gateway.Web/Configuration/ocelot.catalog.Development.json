{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/Catalog",
      "DownstreamHostAndPorts": [
        {
          "Host": "machinecartsystem.catalog.api",
          "Port": 80
        }
      ],
      "DownstreamHttpMethod": "Get",
      "UpstreamPathTemplate": "/catalog-api1/Catalog",
      "UpstreamHttpMethod": [ "Get" ],
      "SwaggerKey": "catalog",
