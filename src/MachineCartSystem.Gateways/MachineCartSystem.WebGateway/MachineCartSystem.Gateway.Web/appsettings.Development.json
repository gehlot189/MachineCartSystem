{
  "GatewayUrl": "GatewayUrl",
  "IdentityServerUrl": "IdentityServerUrl",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Signing": {
      "PrivateKey": "PrivateKey"
    },
    "Audiences": "Audiences",
    "Issuer": "Issuer",
    "Authority": "Authority",
    "Scopes": "Scopes"
  },
  "DbConfiguration": {
    "ConnectionString": "Server=MYPC;Database=MachineCartSystem.v1;Trusted_Connection=True;",
    "Attempt": {
      "MaxRetryCount": 3,
      "MaxRetryDelayInSeconds": 3
    }
  },
  "OpenIdConfiguration": {
    "StsServer": "{stsServer}",
    "RedirectUrl": "{redirectUrl}/auth-callback",
    "PostLogoutRedirectUri": "{postLogoutRedirectUri}/login",
    "ClientId": "angular",
    "Scope": "{scope}",
    "ResponseType": "code",
    "SilentRenew": true,
    "SilentRenewUrl": "{silentRenewUrl}/silent-refresh",
    "LogLevel": "Warn",
    // startCheckSession: true,
    // historyCleanupOff: false,
    "PostLoginRoute": "/",
    "ForbiddenRoute": "/forbidden",
    "unauthorizedRoute": "/unauthorized",
    //maxIdTokenIatOffsetAllowedInSeconds: 40,
    "RenewTimeBeforeTokenExpiresInSeconds": 10,
    "AutoUserinfo": true,
    "UseRefreshToken": false
  }
}
