{
  "GatewayUrl": "http://localhost:5001",
  "IdentityServerUrl": "{IdentityServerUrl}",
  "Api": {
    "name": "Basket Api",
    "host": "{BasketUrl}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Signing": {
      "PrivateKey": "MySecretKey"
    },
    "Audiences": "basket",
    "Issuer": "{Issuer}",
    "Authority": "{Authority}",
    "Scopes": "basket"
  },
  "DbConfiguration": {
    "ConnectionString": "Server=MYPC;Database=MachineCartSystem.v1.Basket;Trusted_Connection=True;",
    "Attempt": {
      "MaxRetryCount": 3,
      "MaxRetryDelayInSeconds": 3
    }
  }
}
